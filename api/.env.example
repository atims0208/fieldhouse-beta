# Server Configuration
PORT=3001
NODE_ENV=development

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=fieldhouse

# JWT Authentication
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=1d
JWT_REFRESH_SECRET=your_refresh_token_secret_here
JWT_REFRESH_EXPIRES_IN=7d

# Bunny.net Streaming Configuration
BUNNY_API_KEY=your_bunny_api_key_here
BUNNY_STREAM_LIBRARY_ID=your_bunny_stream_library_id
BUNNY_PULL_ZONE_URL=your_bunny_pull_zone_url

# Security
CORS_ORIGIN=http://localhost:3000
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100

# Logging
LOG_LEVEL=info

# Lovense API Configuration
LOVENSE_TOKEN=your-lovense-token-here
LOVENSE_KEY=your-lovense-key-here
LOVENSE_IV=your-lovense-iv-here
